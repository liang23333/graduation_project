net: "test_imagelabelmapdata.prototxt"
test_iter: 0
test_interval: 100
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 1e-6
lr_policy: "step"
gamma: 0.1
iter_size: 1
# stepsize should also be lower, as we're closer to being done
stepsize: 1000
display: 1000
average_loss: 50
max_iter: 100
momentum: 0.9
weight_decay: 0.0002


# uncomment the following to default to CPU mode solving
#solver_mode: GPU
#device_id:0
